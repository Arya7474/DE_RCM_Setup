{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "HOSRCMDEVADF4321"
		},
		"HOSRCMDEVLAKE_ls_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'HOSRCMDEVLAKE_ls'"
		},
		"HOSRCMDEVADLS_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://hosrcmstorageadls21345.dfs.core.windows.net/"
		},
		"HOSRCMDEVSQL_ls_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "hosrcmsourcesqlserver427371.database.windows.net"
		},
		"HOSRCMDEVSQL_ls_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "@{linkedService().db_name}"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "HOSRCMDEVADF4321_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"HOSRCMDEVLAKE_ls_accessToken": {
						"value": "[parameters('HOSRCMDEVLAKE_ls_accessToken')]"
					},
					"HOSRCMDEVADLS_ls_properties_typeProperties_url": {
						"value": "[parameters('HOSRCMDEVADLS_ls_properties_typeProperties_url')]"
					},
					"HOSRCMDEVSQL_ls_properties_typeProperties_server": {
						"value": "[parameters('HOSRCMDEVSQL_ls_properties_typeProperties_server')]"
					},
					"HOSRCMDEVSQL_ls_properties_typeProperties_database": {
						"value": "[parameters('HOSRCMDEVSQL_ls_properties_typeProperties_database')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}